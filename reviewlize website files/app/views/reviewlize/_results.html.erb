<% results["results"]["aspect_analize"].each do |k,v| %>
  <div class="results-aspect-container">
    <span class="results-aspect"><%= k %></span>
    <div class="progress bg-danger my-2">
      <div class="progress-bar bg-success" role="progressbar" style="font-weight: bold;width: <%= v["positive"] == 0 && v["negative"] == 0 ? 50 : v["positive"].to_f/(v["positive"].to_f+v["negative"].to_f)*100  %>%;" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"><%=(v["positive"].to_f > 0 || (v["negative"].to_f == 0 && v["positive"].to_f == 0)) ? "+" : ""%></div>
    </div>
  </div>
<% end %>